/**
 * 16-bit multiplexor:
 * for i = 0..15 out[i] = a[i] if sel == 0
 *                        b[i] if sel == 1
 */

CHIP Mux16 {
    IN a[16], b[16], sel;
    OUT out[16];

    PARTS:
    Mux8(a=a[0..7], b=b[0..7], sel=sel, out=out[0..7]);
    Mux8(a=a[8..15], b=b[8..15], sel=sel, out=out[8..15]);
}
